{
  "compilerOptions": {
    "target": "ES2019",
    "outDir": "dist",
    "module": "commonjs",
    "resolveJsonModule": true,
    "removeComments": true,
    "declaration": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2019"]
  },
  "include": ["./src/**/*"],
  "exclude": ["**/*.spec.ts"]
}
